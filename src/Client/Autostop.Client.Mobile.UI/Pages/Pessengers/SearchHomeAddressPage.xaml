<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:views="clr-namespace:Autostop.Client.Mobile.UI.Views;assembly=Autostop.Client.Mobile.UI"
             xmlns:helpers="clr-namespace:Autostop.Client.Mobile.UI.Helpers;assembly=Autostop.Client.Mobile.UI"
             x:Class="Autostop.Client.Mobile.UI.Pages.Pessengers.SearchHomeAddressPage"
             BackgroundColor="#F5F5F5"
             helpers:PageHelper.BarTintColor="#F5F5F5">
    <ContentPage.Content>
        <views:AddressesListView ItemsSource="{Binding SearchResults}" SelectedItem="{Binding SelectedSearchResult}"/>
    </ContentPage.Content>
</ContentPage>